<%= render :partial => 'layouts/model_info/header' %>
<% page=params['page']%>
<h3>View Your Associated Data</h3>
<table class="table table-hover table-bordered">
  <tr>
    <th>Attribute</th>
    <th>Value</th>
  </tr>
  <% @relational_model_class.column_names.each do |k| %>
      <tr>
        <th><%= k %></th>
        <td><%= @single_relational_data.send(k) %></td>
      </tr>
  <% end %>
  <tr>
    <th>Actions</th>
    <td>
      <%= link_to 'back', :back %>
    </td>
  </tr>
</table>