<h1>View Your Data</h1>
<table class="table table-hover table-bordered">
  <tr>
    <th>Attribute</th>
    <th>Value</th>
  </tr>
  <% @resource.column_names.each do |k| %>
      <tr>
        <th><%= k %></th>
        <td><%= @model_data_resource.send(k) %></td>
      </tr>
  <% end %>
  <tr>
    <th>Actions</th>
    <td>
      <%= link_to 'back', fetch_model_infos_index_path %>
    </td>
  </tr>
</table>